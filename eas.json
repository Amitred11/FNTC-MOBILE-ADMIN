{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_BASE_URL": "${secret.API_BASE_URL}",
        "CONFIG_INTERNAL_API_KEY": "${secret.CONFIG_INTERNAL_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}